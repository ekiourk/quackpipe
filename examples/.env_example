# .env
# This file contains secrets for the sources defined in config.yml
# It can be loaded directly by quackpipe's session and utilities.

# === Secrets for secret_name: "pg_prod" ===
PG_PROD_HOST=localhost
PG_PROD_PORT=5432
PG_PROD_DATABASE=your_database_name
PG_PROD_USER=your_postgres_user
PG_PROD_PASSWORD=your_postgres_password

# === Secrets for secret_name: "aws_datalake" ===
AWS_DATALAKE_ACCESS_KEY_ID=YOUR_AWS_ACCESS_KEY
AWS_DATALAKE_SECRET_ACCESS_KEY=YOUR_AWS_SECRET_KEY

# === Secrets for Azure Connection String ===
AZURE_PROD_CS_CONNECTION_STRING="DefaultEndpointsProtocol=https;AccountName=...;AccountKey=...;EndpointSuffix=core.windows.net"

# === Secrets for Azure Service Principal ===
AZURE_PROD_SP_TENANT_ID="your-tenant-id"
AZURE_PROD_SP_CLIENT_ID="your-client-id"
AZURE_PROD_SP_CLIENT_SECRET="your-client-secret"